	<style>
        @font-face {
      font-family: "Kodchasan-Regular";
  src: url('/font/Kodchasan-Regular.ttf') format('truetype');
    }

    body {
      font-family: "Kodchasan-Regular", sans-serif !important;
    }
	@media print {
		.table td, .table th {
