                    <td class="text-center">:</td>
                        <td style="background-color: rgb(234, 234, 234);"><%= data.team2 %></td>
                </tr>
                </table>
                  <% for(var j =0;j<6;j++){ %>
                    <table class="table table-bordered">
                      <thead >
                        <tr class="border-dark">
                          <td></td>
                          <% for(var i=0;i<30;i++){ %>
                            <th width="50px" class="text-center"><%= i+1 %></th>
                            <% } %>
                            <th class="text-center" width="200px">รวม</th>
                        </tr>
                        <tr class="border-dark">
                          <td width="1000px" height="30px"><%= data.p1 %></td>
                          <% for(var i=0;i<30;i++){ %>
                            <th width="50px"></th>
                            <% } %>
                            <th class="text-center" width="200px"></th>
                        </tr>
                        <tr class="border-dark">
                          <td width="300px" height="30px"><%= data.p2 %></td>
                          <% for(var i=0;i<30;i++){ %>
                            <th width="50px"></th>
                            <% } %>
