                        </tr>
                        <tr class="border-dark" style="background-color: rgb(234, 234, 234);">
                          <td width="150px" height="30px"><%= data.p3 %></td>
                          <% for(var i=0;i<30;i++){ %>
                            <th width="50px"></th>
                            <% } %>
                            <th class="text-center" width="200px"></th>
                        </tr>
                        <tr class="border-dark" style="background-color: rgb(234, 234, 234);">
                          <td width="150px" height="30px"><%= data.p4 %></td>
                          <% for(var i=0;i<30;i++){ %>
                            <th width="50px"></th>
                            <% } %>
                            <th class="text-center" width="200px"></th>
                        </tr>
                      </thead>
                    </table>
                    <% } %>

                    
                  
