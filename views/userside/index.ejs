<%- include("../partials/header") %>
        <div class="col-10 py-3">
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="container text-center">
    <h1>USER INDEX</h1>
    

    <div class="card">
        <div class="card-header" style="border: solid;">
            <h3>การแข่งขันที่ดำเนินการอยู่</h3><a class="btn btn-warning" href="/showall">ดูทั้งหมด</a>
        </div>

            <div class="d-flex justify-content-center flex-wrap">
                <% if(data.length) { %>
                    <% for(let i = 0; i < 4; i++) { %>
                        <% if(data && data[i]){ %>
                    <div class="card" style="width: 300px; height: 500px;">
                <a href="/tnmdetail/<%= data[i].tnmID %>">
                    <div class="card-header"><%= data[i].tnmName %></div>
                    <img src="images/<%= data[i].tnmPicture %>" style="height: 300px; width: 300px;">
                    <div class="card-body"><%= data[i].tnmStartdate %></div>
                </a>
                    </div>

                    <% } %>
                <% } %>
            <% } %>

        <% if(!data.length) { %>
            <p class="text-center">ไม่พบรายการแข่งขัน</p>
        <% } %>
    </div>
    </div>
</body>
</html>
</div>